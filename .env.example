# API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here  # For embeddings (text-embedding-3-small)

# Backend
BACKEND_PORT=8000
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Frontend
VITE_API_URL=http://localhost:8000

# TypeDB (Knowledge Graph)
TYPEDB_HOST=localhost
TYPEDB_PORT=1729
TYPEDB_DATABASE=dsrp_canvas
TYPEDB_USERNAME=admin
TYPEDB_PASSWORD=password

# PostgreSQL + pgvector (Vector Search)
POSTGRES_URL=postgresql://dsrp:dsrp_password@localhost:5432/dsrp_canvas
EMBEDDING_MODEL=text-embedding-3-small

# Redis (Cache)
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=300  # Default cache TTL in seconds

# File Storage
UPLOAD_DIR=./uploads
MAX_FILE_SIZE_MB=100
